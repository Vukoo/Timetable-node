                                  <th>Imie</th>
                                  <th>Nazwisko</th>
                                  <th>Kontakt</th>
                                </tr>
                              </thead>
                              <tbody>
                                    
                                            <% for(var i=0; i < todos.length; i++){ %>
                                                <tr>
                                                <td> <%= todos[i].firstName %> </td>
                                                <td> <%= todos[i].lastName %> </td>
                                                <td> <%= todos[i].phone %> </td>
                                            </tr>
                                                <% } %>
                                </tbody>
                                </table>
                        </div>
                </div>
